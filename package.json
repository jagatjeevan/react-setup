{
  "name": "promo-frontend",
  "version": "1.0.0",
  "description": "promotion frontend ",
  "main": "index.js",
  "engines": {
    "node": "=11.2.0",
    "npm": "=6.4.1"
  },
  "sasslintConfig": ".sass-lint.yml",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/helpers.js --require ./test/dom.js --recursive test/**/*.test.js",
    "test:watch": "npm test -- --watch",
    "clean-nyc": "rimraf .nyc_output",
    "clean-build": "rimraf build",
    "clean-coverage": "rimraf coverage",
    "clean": "npm run clean-nyc && npm run clean-build && npm run clean-coverage",
    "build": "webpack",
    "coverage": "nyc --reporter=html mocha --compilers js:babel-core/register --require ./test/helpers.js --require ./test/dom.js --recursive test/**/*.test.js",
    "sassLint": "sass-lint -c .sass-lint.yml 'app/**/*.scss' -v -q",
    "start": "npm run clean && npm run build && webpack-dev-server --mode='development' --open",
    "production": "webpack -p",
    "lint": "eslint .eslintrc.json app/js/",
    "lint:fix": "eslint .eslintrc.json app/js/ --fix",
    "validate": "npm ls"
  },
  "pre-commit": [
    "sassLint",
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:jagatjeevan/react-setup.git"
  },
  "author": {
    "name": "Jagat Jeevan Sahoo",
    "email": "kanhajeevan@gmail.com"
  },
  "license": "MIT License",
  "private": true,
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^24.1.0",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^13.2.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.11.0",
    "nyc": "^13.2.0",
    "precommit-hook": "^3.0.0",
    "react-addons-test-utils": "^15.6.2",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^2.6.3",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "i18next": "^19.5.5",
    "i18next-browser-languagedetector": "^2.2.4",
    "i18next-xhr-backend": "^1.5.1",
    "js-cookie": "^2.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-i18next": "^10.0.1",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  }
}
