{
  "name": "promo-frontend",
  "version": "1.0.0",
  "description": "promotion frontend ",
  "main": "index.js",
  "engines": {
    "node": "=6.11.1",
    "npm": "=3.10.10"
  },
  "sasslintConfig": ".sass-lint.yml",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/helpers.js --require ./test/dom.js --recursive test/**/*.test.js",
    "test:watch": "npm test -- --watch",
    "clean-nyc": "rimraf .nyc_output",
    "clean-build": "rimraf build",
    "clean-coverage": "rimraf coverage",
    "clean": "npm run clean-nyc && npm run clean-build && npm run clean-coverage",
    "build": "webpack",
    "coverage": "nyc --reporter=html mocha --compilers js:babel-core/register --require ./test/helpers.js --require ./test/dom.js --recursive test/**/*.test.js",
    "sassLint": "sass-lint -c .sass-lint.yml 'app/**/*.scss' -v -q",
    "start": "npm run clean && npm run build && webpack-dev-server --open",
    "production": "webpack -p",
    "lint": "eslint .eslintrc.json app/js/",
    "lint:fix": "eslint .eslintrc.json app/js/ --fix",
    "nsp": "nsp check --output checkstyle",
    "validate": "npm ls"
  },
  "pre-commit": [
    "sassLint",
    "lint",
    "test",
    "nsp"
  ],
  "repository": {
    "type": "git",
    "url": "git@git.mckinsey-solutions.com:promo/promo-frontend.git"
  },
  "author": "",
  "license": "Copyright McKinsey",
  "private": true,
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "9.12.0",
    "mocha": "^3.5.0",
    "node-sass": "^4.5.3",
    "nsp": "^2.7.0",
    "nyc": "^11.1.0",
    "precommit-hook": "^3.0.0",
    "react-addons-test-utils": "^15.6.0",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.6",
    "sinon": "1.17.7",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "i18next": "^8.4.3",
    "i18next-browser-languagedetector": "^2.0.0",
    "i18next-xhr-backend": "^1.4.2",
    "js-cookie": "^2.1.4",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-i18next": "^4.8.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
